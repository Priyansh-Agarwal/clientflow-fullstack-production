{
  "version": 2,
  "name": "clientflow-ai-suite",
  "builds": [
    {
      "src": "api-server/index-production.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api-server/index-production.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api-server/index-production.js"
    },
    {
      "src": "/health",
      "dest": "/api-server/index-production.js"
    },
    {
      "src": "/test",
      "dest": "/api-server/index-production.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api-server/index-production.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api-server/index-production.js"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  }
}